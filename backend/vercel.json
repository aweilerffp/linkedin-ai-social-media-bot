{
  "version": 2,
  "builds": [
    {
      "src": "webhook-vercel.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "/webhook-vercel.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/webhook-vercel.js"
    },
    {
      "src": "/(.*)",
      "dest": "/webhook-vercel.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "webhook-vercel.js": {
      "maxDuration": 30
    }
  }
}